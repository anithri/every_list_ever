<div id="<%= dom_id profile %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Handle:</strong>
    <%= profile.handle %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Avatar:</strong>
    <%= profile.avatar %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Description:</strong>
    <%= profile.description %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Location:</strong>
    <%= profile.location %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Visibility:</strong>
    <%= profile.visibility %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">User:</strong>
    <%= profile.user_id %>
  </p>

</div>
